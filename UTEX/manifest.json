{
    "manifest_version": 2,
    "name": "UTexas Launcher",
    "description": "Quickly launch University of Texas Media",
    "version": "1.0.0",
    "icons": {
        "128": "utex_size_128.png"
    },
    "browser_action": {
        "default_icon": "utex_base.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}